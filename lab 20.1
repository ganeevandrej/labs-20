#include <iostream>

using namespace std;
int main()
{
    int a[10], b[10], c[10];
    int n;
    cin >> n;

    int i;
    for (i = 0; i < n; ++i) {
        cin >> a[i];
    }

    int k = 0;
    b[k] = 1;
    c[k] = a[k];
    for (i = 1; i < n; ++i) {
        if (a[i - 1] == a[i]) ++b[k];
        else {
            ++k;
            b[k] = 1;
            c[k] = a[i];
        }
    }
    for (i = 0; i <= k; ++i)
        cout << b[i] << " ";
    cout << endl;
    for (i = 0; i <= k; ++i)
        cout << c[i] << " ";
    return 0;
}
